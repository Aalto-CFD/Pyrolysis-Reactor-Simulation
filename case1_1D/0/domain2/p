FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/domain2";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
pInitial       1e5;

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $pInitial;

boundaryField
{
	".*"
            {
                type            calculated;
                value           uniform $pInitial;
            }	
                
	"front|back|top|bottom"
 		{
 			type		empty; 
 		}
}

