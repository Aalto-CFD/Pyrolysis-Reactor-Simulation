FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type                          heSolidThermo;
    mixture                     pureMixture;
    transport                  constIso;
    thermo                      hConst;
    equationOfState      rhoConst;
    specie                        specie;
    energy                       sensibleEnthalpy;
}
//dry wood
mixture
{
    specie
    {
        molWeight   150;
    }
    transport
    {
        kappa   0.2;
    }
    thermodynamics
    {
        Hf      0;
        Cp      1500;
    }
    equationOfState
    {
        rho     600;
    }
}


