FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/isolant";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    w_ext_convec-isolant
    {
        type            zeroGradient;
    }
    isolant_to_tube
    {
        type            calculated;
        value           uniform 0;
    }
    isolant_to_fluid_cavity
    {
        type            calculated;
        value           uniform 0;
    }
}


