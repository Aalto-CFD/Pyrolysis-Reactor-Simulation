FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/fluid_ar";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{

	fluid_ar_to_tube	// interface
	{
		type            compressible::kqRWallFunction;
		value           uniform 0.1;
	}

	fluid_out		// outlet 
	{		
		type            fixedValue;
		value           uniform 0.1;
	} 

	fluid_in		// inlet
	{
		type            inletOutlet;
		value           uniform 0.1;
		inletValue      uniform 0.1;
	} 

}

