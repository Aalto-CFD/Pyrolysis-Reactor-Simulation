FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/fluid_ar";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{


	fluid_ar_to_tube	// interface
	{
		type            compressible::epsilonWallFunction;
		value           uniform 0.01;
	}

	fluid_out		// outlet 
	{		
		type            calculated;
		value           uniform 0.01;
	} 

	fluid_in		// inlet
	{
		type            calculated;
		value           uniform 0.01;
	} 

}
