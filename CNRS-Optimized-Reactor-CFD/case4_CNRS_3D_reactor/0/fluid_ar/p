FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/fluid_ar";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
pInitial       1e5;

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $pInitial;

boundaryField
{

	fluid_ar_to_tube	// interface
	{
		type            calculated;
		value           uniform $pInitial;
	}
//---------------------------------------------------------------------------------------
	fluid_out		// outlet 
	{		
		type            calculated;
		value           uniform $pInitial;
	} 
//---------------------------------------------------------------------------------------
	fluid_in		// inlet
	{
		type            calculated;
		value           uniform $pInitial;
	} 

}
