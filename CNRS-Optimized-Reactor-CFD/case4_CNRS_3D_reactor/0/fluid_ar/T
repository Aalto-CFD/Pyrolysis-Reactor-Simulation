FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/fluid_ar";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
Tinitial	300;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinitial;


boundaryField
{

	fluid_ar_to_tube	// interface
	{
                type            compressible::turbulentTemperatureRadCoupledMixed;
                Tnbr            T;
                kappaMethod     fluidThermo;
                qrNbr           none;
                qr              qr;
                kappa           none;
                thermalInertia  true;
                value           uniform $Tinitial;
	}
//---------------------------------------------------------------------------------------
	fluid_out	// outlet 
	{
		type            inletOutlet;
		value           uniform $Tinitial;
		inletValue      uniform $Tinitial;
	} 
//---------------------------------------------------------------------------------------
	fluid_in	// inlet
	{
		type            fixedValue;
		value           uniform $Tinitial;
	} 

}

// ************************************************************************* //
