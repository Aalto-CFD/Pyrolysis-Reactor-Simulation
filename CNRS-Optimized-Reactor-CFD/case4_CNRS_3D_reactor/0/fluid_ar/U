FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0/fluid_ar";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
Uinlet          (0 0 -0.02);

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

	fluid_ar_to_tube	// interface
	{
		type            fixedValue;
		value           uniform (0 0 0);
	}
//---------------------------------------------------------------------------------------
	fluid_out	// outlet 
	{
             	type            inletOutlet;
                value           uniform $Uinlet;
                inletValue      uniform $Uinlet;

	} 
//---------------------------------------------------------------------------------------
	fluid_in	// inlet
	{
		type            fixedValue;
		value           uniform $Uinlet;
	} 
	
}
