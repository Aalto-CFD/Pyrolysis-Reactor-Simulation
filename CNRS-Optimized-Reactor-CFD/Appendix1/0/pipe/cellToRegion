FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/pipe";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    leftLet
    {
        type            zeroGradient;
    }
    rightLet
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    pipe_to_bottomSolid
    {
        type            calculated;
        value           uniform 0;
    }
    pipe_to_topSolid
    {
        type            calculated;
        value           uniform 0;
    }
}


